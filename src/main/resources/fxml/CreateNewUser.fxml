<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="122.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ryhma10.tilastoohjelma.view.CreateNewUserController">
    <children>
        <Pane layoutY="2.0" prefHeight="118.0" prefWidth="344.0">
            <children>
                <ButtonBar layoutX="124.0" layoutY="77.0" prefHeight="40.0" prefWidth="168.0">
                    <buttons>
                        <Button mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
                        <Button mnemonicParsing="false" onAction="#handleDone" text="Done" />
                    </buttons>
                </ButtonBar>
                <Label layoutX="14.0" layoutY="23.0" text="Profile name" />
                <Label layoutX="14.0" layoutY="56.0" text="Password" />
                <TextField fx:id="createProfileNameField" layoutX="91.0" layoutY="19.0" onKeyPressed="#onEnter" prefHeight="25.0" prefWidth="213.0" />
                <TextField fx:id="createProfilePasswordField" layoutX="91.0" layoutY="52.0" onKeyPressed="#onEnter" prefHeight="25.0" prefWidth="213.0" />
            <ProgressIndicator fx:id="progressIndicator" layoutX="310.0" layoutY="85.0" prefHeight="25.0" prefWidth="25.0" />
            </children>
        </Pane>
    </children>
</AnchorPane>
